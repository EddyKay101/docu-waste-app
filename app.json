{
  "expo": {
    "name": "docu-waste",
    "slug": "docu-waste",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "backgroundColor": "#cfcfd4"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#311b92"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.docuwaste.docuwaste",
      "adaptiveIcon": {
        "backgroundColor": "#cfcfd4"
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "svg"
      ]
    }
  }
}